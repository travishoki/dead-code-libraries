{
  "entry": ["./src/index.tsx"],
  "extensions": [".ts", ".tsx"],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts",
    "**/tests/**",
    "**/*.test.tsx",
    "**/stories/**",
    "**/**.stories.**",
    "**/**.generated.**",
    "**/queries.ts",
    "**/__mocks__/**",
    "**/mocks.ts",
    "**/mock.ts",
    "**/testData.ts",
    "**/storyComponent.**",
    "**/enumerations/**"
  ],
  "aliases": {
    "@app/designer": "./src/scenes/in_app/designer",
    "actions": "./src/redux/actions",
    "design_system": "./src/design_system",
    "enumerations": "./src/enumerations",
    "files": "./src/files",
    "images": "./src/images",
    "reducers": "./src/redux/reducers",
    "routes": "./src/routes",
    "scenes": "./src/scenes",
    "services": "./src/services",
    "shared": "./src/shared",
    "styles": "./src/styles",
    "src": "./src",
    "types": "./types",
    "utilities": "./src/utilities"
  },
  "ignoreUnresolved": ["./i18n/i18n", "./team_upload_example.xlsx?url"],
  "ignoreUnused": [
    "@datadog/datadog-ci",
    "@faker-js/faker",
    "@storybook/addon-actions",
    "@tanstack/react-table",
    "@types/react-scroll-to-bottom",
    "@types/scroll-into-view",
    "bootstrap",
    "color",
    "core-js",
    "cypress-real-events",
    "improved-yarn-audit",
    "jsonwebtoken",
    "minimatch",
    "nanoid",
    "react-router-cache-route"
  ]
}
